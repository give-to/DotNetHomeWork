/*-------------------------------------------------------------*/
//显示128*64图片
//执行效率高
void img12864_disp(unsigned char code *img)
{
    unsigned char i,j;
	for(j=0;j&lt;8;j++)
	{
	    LCDSel(1);
		WriteCmdToLCD12864(LCD_ON);
		SetX(j);
		SetZ(0);
		SetY(0);
		for(i=0;i&lt;64;i++)
		   WriteDatToLCD12864(img[j*128+i]);
		LCDSel(2);
		WriteCmdToLCD12864(LCD_ON);
		SetX(j);
		SetZ(0);
		SetY(0);
		for(i=64;i&lt;128;i++)
		   WriteDatToLCD12864(img[j*128+i]);
	}
}	

//亦木明 2008.2.23
//显示一个16X32的字符
void Show16X32(unsigned char line,unsigned char column, unsigned char code *pt,bit flag)
{
	unsigned char i,column_temp1,column_temp2,column_temp3;
	column_temp1=column;
	column_temp2=column;
	column_temp3=column;
	LCDSel(1);                         //选通左屏
	SetY(column);                      //设置起始列
	SetX(line);                          //设置行
	for(i=0;i&lt;16;i++)
	{
		if(column&gt;=64)                      //如果列&gt;=64，则选通右屏
		{
			LCDSel(2);
			column=column%64;               //设置右屏起始列
			SetY(column);
			SetX(line);
		}
		if(flag)	
		WriteDatToLCD12864(*pt);                     //写数据
		else
		WriteDatToLCD12864(~(*pt)); //colinluan 080729
		pt++;
		column++;
	}
	LCDSel(1);
	SetX(line+1);
	column=column_temp1;
	SetY(column);
	for(i=0;i&lt;16;i++)
	{
		if(column&gt;=64)
		{
			LCDSel(2);
			column=column%64;
			SetY(column);
			SetX(line+1);
		}	
		if(flag)	
		WriteDatToLCD12864(*pt);                     //写数据
		else
		WriteDatToLCD12864(~(*pt)); //colinluan 080729
		pt++;
		column++;
	}
	LCDSel(1);
	SetX(line+2);
	column=column_temp2;
	SetY(column);
	for(i=0;i&lt;16;i++)
	{
		if(column&gt;=64)
		{
			LCDSel(2);
			column=column%64;
			SetY(column);
			SetX(line+2);
		}	
		if(flag)	
		WriteDatToLCD12864(*pt);                     //写数据
		else
		WriteDatToLCD12864(~(*pt)); //colinluan 080729
		pt++;
		column++;
	}
	LCDSel(1);
	SetX(line+3);
	column=column_temp3;
	SetY(column);
	for(i=0;i&lt;16;i++)
	{
		if(column&gt;=64)
		{
			LCDSel(2);
			column=column%64;
			SetY(column);
			SetX(line+3);
		}	
		if(flag)	
		WriteDatToLCD12864(*pt);                     //写数据
		else
		WriteDatToLCD12864(~(*pt)); //colinluan 080729
		pt++;
		column++;
	}
}
/*-------------------------------------------------------*/用12864进行制作万年历</p></div></a></li><li class="pub_recom_item download"><a class="pr_item" href=https://download.csdn.net/download/weixin_38524871/15932247 target="_blank" data-report-view={"mod":"popu_595","extra":"{\"utm_medium\":\"distribute.pc_relevant_download.none-task-download-2~default~OPENSEARCH~default-13.nonecase\"}","dist_request_id":"1620275042504_12218","index":"13","strategy":"2~default~OPENSEARCH~default"} data-report-click={"mod":"popu_595","extra":"{\"utm_medium\":\"distribute.pc_relevant_download.none-task-download-2~default~OPENSEARCH~default-13.nonecase\"}","dist_request_id":"1620275042504_12218","index":"13","strategy":"2~default~OPENSEARCH~default"} data-report-query=utm_medium=distribute.pc_relevant_download.none-task-download-2~default~OPENSEARCH~default-13.nonecase&depth_1-utm_source=distribute.pc_relevant_download.none-task-download-2~default~OPENSEARCH~default-13.nonecase><div class="pr_item_l"><div class="fix"><img src=//g.csdnimg.cn/pub-recom/img/type//unknow.svg alt=""><span>38KB</span></div> </div><div class="pr_item_r"><p class="pr_i_t">FullFill Ali <em>Adress</em>-crx插件</p><span class="pr_item_t_r fr"><span class="pr_t_date">2021-03-19</span></span><p class="pr_i_d">FullFill Ali地址
