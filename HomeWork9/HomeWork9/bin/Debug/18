<link rel="stylesheet" href="//csdnimg.cn/release/download/static_files/pc/css/float_box-f507429dbc.css" />
<div class="float_box">
    
</div>
<script>
    $(function () {
        function fixBox() {
            var el = document.querySelector('.float_box')
            var navEl = document.querySelector('#navs')
            var sLeft = window.pageXOffset || document.documentElement.scrollLeft || document.body.scrollLeft || 0
            // 跟nav左边同步
            var navLeft = navEl.offsetLeft - sLeft
            var boxTop = $('#csdn-toolbar').height() + $('#csdn-nav-second').height() + 18
            el.style.left = navLeft + 'px'
            el.style.top = boxTop + 'px'
        }
        setTimeout(function () {
            fixBox()
        }, 300)
        document.addEventListener('scroll', function(){
            fixBox()
        })
        window.addEventListener('resize', function () {
            fixBox()
        })

        var storage = window.localStorage.getItem('hideFloat')
        if (!storage || storage !== getCurrentDay()) {
            $('.float_box').show()
        }
        $('.float_box_close').on('click', function () {
            var clickTime = getCurrentDay()
            window.localStorage.setItem('hideFloat', clickTime)
            $('.float_box').hide()
        })
        function getCurrentDay () {
            return new Date().getFullYear() + '/' + (new Date().getMonth() + 1) + '/' + new Date().getDate()
        }
    })
</script>
